<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zwanzigdrei.localfoodie">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_chillicrab"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_chillicrab_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <activity android:name="com.example.zwanzigdrei.localfoodie.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.zwanzigdrei.localfoodie.NextActivity"/>
        <activity android:name="com.example.zwanzigdrei.localfoodie.Function2Activity"/>
        <activity android:name="com.example.zwanzigdrei.localfoodie.Function3Activity"
            android:windowSoftInputMode="stateAlwaysHidden"/>
        <activity android:name="com.example.zwanzigdrei.localfoodie.secondpage"/>
        <activity android:name="com.example.zwanzigdrei.localfoodie.OverallMap"/>

    </application>

</manifest>